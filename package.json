{
  "name": "@voyce/bun-env",
  "type": "module",
  "version": "1.0.1",
  "private": false,
  "description": "Type-safe environment variable management with Zod validation, dotenv loading, and variable expansion for Bun applications",
  "author": "Voyce Labs",
  "license": "MIT",
  "homepage": "https://github.com/voycelabs/bun-env#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/voycelabs/bun-env.git"
  },
  "bugs": {
    "url": "https://github.com/voycelabs/bun-env/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "environment",
    "env",
    "config",
    "configuration",
    "zod",
    "validation",
    "dotenv",
    "bun",
    "type-safe",
    "schema"
  ],
  "exports": {
    ".": "./src/index.ts"
  },
  "module": "src/index.ts",
  "types": "src/index.ts",
  "engines": {
    "bun": ">=1.3"
  },
  "scripts": {
    "lint": "bunx biome lint .",
    "format": "bunx biome format --write .",
    "check": "bunx biome check --write .",
    "ci:check": "bunx biome ci .",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.1",
    "@types/bun": "latest",
    "@voyce/biome-config": "^1.0.2",
    "@voyce/typescript-config": "^1.0.2"
  }
}
